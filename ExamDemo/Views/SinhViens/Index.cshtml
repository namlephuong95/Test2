@model IEnumerable<ExamDemo.Models.SinhVien>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<h4 style="color: brown">Tổng chống đẩy = @ViewBag.ChongDaySL | Tổng tiền phạt = @ViewBag.TienPhatSL</h4>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaSoSV)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HinhThuc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SoLuong)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayPhat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaSoSV)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HinhThuc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SoLuong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayPhat)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
